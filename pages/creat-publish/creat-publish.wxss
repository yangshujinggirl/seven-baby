page {
  background: #F4F4F4;
}
.feedback-wrap {
  margin-top: 20rpx;
}
.feedback-wrap .top-action {
  padding:30rpx 20rpx;
  background-color: #fff;
}
.section .label {
  display: block;
  padding-bottom: 20rpx;
  font-size: 32rpx;
  color: #000;
  border-bottom: 2rpx solid #dadada;
}
.feedback-wrap  .section textarea {
  width: 100%;
  padding:20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #9a9a9a;
}
.file-action {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  background-color: #fff;
}
.pic-item {
  width: 180rpx;
  height: 180rpx;
  border-radius: 12rpx;
  overflow: hidden;
  margin: 0 10rpx 10rpx 0;
}
.upload {
  width: 180rpx;
  height: 180rpx;
  background: #F4F4F4;
  border: 2rpx solid #eee;
  border-radius: 16rpx;
  text-align: center;
  margin-bottom: 20rpx;
  position: relative;
  overflow: hidden;
  vertical-align: middle;
}
.upload image {
  width: 100%;
  height: 100%;
}
.upload .plus-icon {
  font-size: 90rpx;
  color: #999;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.menu-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  padding:30rpx 20rpx;
  margin-top: 20rpx;
}
.menu-item .lf  {
  display: inline-flex;
  align-items: center;
  font-size: 32rpx;
  color: #000;
}
.menu-item .lf image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 16rpx;
}
.menu-item .rf {
  color: #646464;
  font-size: 24rpx;
}
.btn-box {
  padding-bottom: 40rpx;
  width: 90%;
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.btn-box .tips {
  color: #646464;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-bottom: 20rpx;
}
.btn-box .keep{
  height: 82rpx;
  line-height: 82rpx;
  color: #fff;
  background-color: #EB5412;
  border-radius: 8rpx;
  font-size: 32rpx;

}